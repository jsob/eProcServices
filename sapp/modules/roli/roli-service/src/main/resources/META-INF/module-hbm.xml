<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="tj.roli.model.roli" />
	<class name="tj.roli.model.impl.roliImpl" table="sapp.roli">
		<id name="roli_id" type="long">
			<generator class="sequence">
				<param name="sequence">seq_rol</param>
			</generator>
		</id>
		<property name="rol" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>