<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="tj.istorija.popolnenij">
	<namespace>istorija_popolnenij</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
    <entity name="IstorijaPopolnenij" local-service="true" table="sapp.istorija_popolnenij">
        <column name="data_zachislenija" type="Date"></column>
        <column name="istorija_popolnenij_id" type="long" primary="true" id-type="sequence" id-param="seq_ist_popolnie"></column>
        <column name="nomer" type="String"></column>
        <column name="postavwik_id" type="long"></column>
        <column name="put" type="String"></column>
        <column name="summa" type="double"></column>
        <column name="tip" type="String"></column>
        <column type="String" name="tranzakcija"></column>
        <column name="zachislil" type="long"></column>
    </entity>
</service-builder>